{
	"compilerOptions": {
		"declaration": false,
		"jsx": "react",
		"jsxFactory": "h",
		"jsxFragmentFactory": "Fragment",
		"module": "ESNext",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"rootDir": ".",
		"sourceMap": true,
		"strict": true,
		"target": "ESNext",
		"resolveJsonModule": true
	},
	"extends": "@sindresorhus/tsconfig"
}
